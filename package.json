    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource-variable/montserrat": "^5.0.19",
    "@radix-ui/react-dialog": "^1.1.1",
    "@react-pdf/renderer": "^3.4.4",
    "@types/bun": "^1.1.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.6.0",
    "eslint-plugin-astro": "^1.2.3",
    "prettier": "^3.3.3",
